@using MusicOrganizer.Models;
@{
    Layout = "_Layout";
}
<h3>Here are all the records by this artist @Model["artist"]:</h3>

<ol>
@foreach (Record record in @Model["records"])
{
    <li><a href='/artists/@Model["artist"].Id/records/@record.Id'>@record.Title</li>
}
</ol>

<p><a href='/artists'>Return to artists list</a></p>
<p><a href='/'>Return to Main Page</a></p>